<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <form class="" action="/" method="post">

      <input type="text" name="title" value="" placeholder="Title">
      <input type="INTEGER" name="priority" value="" placeholder="Priority">
      <input type="date" name="due" value="" placeholder="Date">
      <input type="text" name="assignee" value="" placeholder="Who Assigneed">
      <input type="submit" name="" value="Add">

    </form>
    {{#model}}
    <ul>
      {{^completed}}
      <li>{{title}}
        <form style="display:inline" action="/complete/{{id}}" method="post">
          <input type="submit" name="complete" value="Completed">
        </form>
        <form style="display:inline" action="/update/{{id}}" method="post">
          <input type="submit" name="edit" value="Edit">
        </form>
      </li>
      </ul>
      {{/completed}}
    {{/model}}
      <h4>Completed</h4>
      {{#model}}
      {{#completed}}
      <ul>
        {{title}}
      </ul>
      {{/completed}}
      {{/model}}
  </body>
</html>
